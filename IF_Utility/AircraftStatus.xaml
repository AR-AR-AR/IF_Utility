<UserControl x:Class="IF_Utility.AircraftStatus"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IF_Utility"
             mc:Ignorable="d" Height="411.883" Width="293.507">
    <Canvas Margin="0,0,32,70">
        <ListView x:Name="listView" Height="401" Width="288" Canvas.Left="3" AlternationCount="2">
            <ListView.View>
                <GridView AllowsColumnReorder="False">
                    <GridViewColumn Header="Parameter" DisplayMemberBinding="{Binding Key}" Width="130"/>
                    <GridViewColumn Header="Reading" DisplayMemberBinding="{Binding Value}" Width="130"/>
                </GridView>
            </ListView.View>


        </ListView>

    </Canvas>
</UserControl>
